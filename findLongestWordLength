function findLongestWordLength(str) {
  var array = [];
  array = str.split(" ");
  var max = array[0];

  for(var word = 0; word < array.length; word++)
  {
      if(array[word] > max)
      {
        max = array[word];
      }
  }
  return max.length;
}

console.log(findLongestWordLength("The quick brown fox jumped over the lazy dog"));
